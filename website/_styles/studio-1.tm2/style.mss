Map { background-color: #222B30; }

#hillshade[class='medium_highlight'] {
  polygon-fill:#f8c;
  polygon-opacity:0.1;
}

#hillshade[class='full_highlight'] {
  polygon-fill:#4ac;
  polygon-opacity:0.2;
}

#hillshade[class='full_shadow'] {
  polygon-fill:#012;
  polygon-opacity:0.6;
}

#hillshade[class='medium_shadow'] {
  polygon-fill:#000;
  polygon-opacity:0.2;
}